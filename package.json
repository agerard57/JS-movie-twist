{
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "eslint": "^8.10.0",
    "eslint-plugin-import": "^2.25.4",
    "express": "^4.17.3",
    "js-unused-exports": "^1.2.1",
    "markdownlint": "^0.25.1",
    "markdownlint-cli": "^0.31.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1"
  },
  "name": "js-my-movie-database",
  "description": "A JavaScript website about vanilla js, ajax, node, express and movies!",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "clean": "rm -rf node_modules",
    "init": "npm run clean && npm i",
    "start": "npx nodemon index.js",
    "check:eslint": "npx eslint --max-warnings 0 .",
    "check:markdownlint": "npx markdownlint-cli .",
    "check:prettier": "npx prettier --check .",
    "check:unused-exports": "npx js-unused-exports ./tsconfig.json",
    "check": "npm run check:markdownlint && npm run check:unused-exports && npm run check:prettier && npm run check:eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agerard57/JS_my-movie-database.git"
  },
  "keywords": [
    "js;website;node;express;ajax;vanilla"
  ],
  "author": "agerard57",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
